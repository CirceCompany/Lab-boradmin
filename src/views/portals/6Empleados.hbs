
            <div class="container mt-5">
                
                <table class="table table-striped mt-3">
                    <thead>
                     
                    <tr>
                      <th scope="col">#</th>
                      <th scope="col">Nombres</th>
                      <th scope="col">Cedula</th>
                      <th scope="col">Direccion</th>
                      <th scope="col">Telefono</th>
                      <th>Fecha Nacimiento</th>
                      <th>Fecha Inicio</th>
                      <th>Departamento</th>
                      <th scope="col"> <a href="/addEmpleado" ><button class="btn btn-success">Agregar Empleado</button></a></th>
                      <th scope="col"></th>
                    </tr>
                   
                    </thead>
                    <tbody>
                    <tr>
                      {{#each empleados}}
                      <tr id="empl-{{id}}">
                        <td>{{id}}</td>
                        <td>{{nombres}}</td>
                        <td>{{cedula}}</td>
                        <td>{{direccion}}</td>
                        <td>{{telefono}}</td>
                        <td>{{fecha_nac}}</td>
                        <td>{{fecha_inicio}}</td>
                        <td>{{departamento}}</td>
                        <td><button class="btn btn-sm btn-success"><a href="/editarEmpleado/{{id}}">Editar</a></button>
                            <button class="btn btn-sm btn-danger"><a href="/delEmpleado/{{id}}">X</a></button></td>
                      </tr>
                      {{/each}}
                    </tr>
                    </tbody>
                  </table>
            </div>